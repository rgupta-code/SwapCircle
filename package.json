{
  "name": "swapcircle",
  "version": "1.0.0",
  "description": "A modern barter marketplace app for exchanging goods and services",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write ."
  },
  "keywords": ["barter", "marketplace", "sustainability", "community", "trading"],
  "author": "SwapCircle Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.41.0",
    "pg": "^8.11.3",
    "knex": "^3.0.1",
    "joi": "^17.11.0",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "socket.io": "^4.7.4",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "openai": "^4.20.1",
    "redis": "^4.6.10",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "@types/node": "^20.9.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
